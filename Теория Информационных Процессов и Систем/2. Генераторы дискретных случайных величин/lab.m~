X = [4, 6, 10, 14, 16, 20, 24];
P = [0.04, 0.1, 0.1, 0.27, 0.33, 0.13, 0.03];
histogram(X, )
%M[X]
M = 0;
for i = 1:7
    M = M + X(i) * P(i);
end
disp 'M[X]';
disp(M);

%D[X]
D = 0;
for i = 1:7
    D = D + P(i) * (X(i) - M) ^ 2;
end
disp 'D[X]';
disp(D);

s = sqrt(D);
E = 2.45 * s / sqrt(7)

%{
%correlation
corr = 1:20;

for i = 1:20
    cov = 0;
    for j = 1:20-i
        cov = cov + (X(j) - M) * (X(j + i) - M);
    end
    cov = cov / (20 - i);
    corr(i) = cov / D;
end

x = linspace(0, 20, 20);
figure
plot(x, corr, '-');

figure;
plot(X(1:999), X(2:1000), '.');


figure;
h = histogram(X);
h.NumBins = 10;
h.Normalization = 'probability';
%}
